# Amadeus API (flights & hotels)
# Sign up at: https://developers.amadeus.com/register
AMADEUS_CLIENT_ID=
AMADEUS_CLIENT_SECRET=

# Reddit API
# Create app at: https://www.reddit.com/prefs/apps
REDDIT_CLIENT_ID=
REDDIT_CLIENT_SECRET=

# Google Maps Platform
# Enable at: https://console.cloud.google.com/google/maps-apis
# Required APIs: Places API, Directions API, Distance Matrix API, Maps JavaScript API
GOOGLE_MAPS_API_KEY=
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=

# Groq API (LLM for Quick Plan chat)
# Sign up at: https://console.groq.com
GROQ_API_KEY=

# Makcorps API (hotel pricing)
# Sign up at: https://www.makcorps.com
MAKCORPS_API_KEY=

# Feature flags
NEXT_PUBLIC_QUICK_PLAN_WIZARD=false
